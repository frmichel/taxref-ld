taxrefsex:Sex
 rdfs:subClassOf skos:Concept;
 rdfs:label "Class of sex"@en;
 rdfs:isDefinedBy <http://taxref.mnhn.fr/lod/taxref-ld>;
 skos:member taxrefsex:Male, taxrefsex:Female.
 
 
# --- Types of Sex

taxrefsex:Male
 a taxrefsex:Sex;
 skos:inScheme <http://taxref.mnhn.fr/lod/taxref-ld>;
 rdfs:label "Male"@en;
 skos:prefLabel "Male"@en;
 skos:prefLabel "Mâle"@fr;
 skos:definition "Gonochoristic organism that can produce male gametes"@en;
 skos:definition "Organisme gonochoristique capable de produire des gamètes mâles"@fr;
 skos:exactMatch <http://purl.obolibrary.org/obo/BILA_0000027>;
 skos:relatedMatch <http://www.wikidata.org/entity/Q44148>.

taxrefsex:Female
 a taxrefsex:Sex;
 skos:inScheme <http://taxref.mnhn.fr/lod/taxref-ld>;
 rdfs:label "Female"@en;
 skos:prefLabel "Female"@en;
 skos:prefLabel "Femelle"@fr;
 skos:definition "Gonochoristic organism that can produce female gametes"@en;
 skos:definition "Organisme gonochoristique capable de produire des gamètes femelles"@fr;
 skos:exactMatch <http://purl.obolibrary.org/obo/BILA_0000028>;
 skos:relatedMatch <http://www.wikidata.org/entity/Q43445>.